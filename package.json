{
  "name": "memory-game",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://javierjulio.github.io/memory-game",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "prepreview": "npm run build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages --dist dist --dotfiles",
    "test": "vitest run && playwright test --fully-parallel",
    "test:e2e": "playwright test --browser all --fully-parallel",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.1.3",
    "@vitest/coverage-c8": "^0.26.0",
    "@vitest/ui": "^0.26.1",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.3",
    "svelte": "^3.55.0",
    "vite": "^4.0.2",
    "vitest": "^0.26.1"
  },
  "dependencies": {
    "dexie": "^3.2.2"
  }
}
